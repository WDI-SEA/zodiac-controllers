<h1><%= element %></h1>

<h2>The Signs:</h2>
<ul>
    <% signs.forEach(sign => { %>
        <li><a href='/<%-element.toLowerCase()%>/<%-sign.toLowerCase()%>'><%= sign %></a></li>
    <% }) %>
</ul>

<h2>Traits</h2>

<ul>
    <% traits.forEach(trait => { %>
        <li><%= trait %></li>
    <% }) %>
</ul>