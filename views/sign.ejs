<div class="zodiac-info">
  <h2><%= title %></h2>

  <div class="signs">
    <ul>
      <% signs.forEach((sign) => { %>
      <li><%= sign %></li>
      <% }) %>
    </ul>
  </div>

  <div class="traits">
    <ul>
      <% traits.forEach((trait) => { %>
      <li><%= trait %></li>
      <% }) %>
    </ul>
  </div>
</div>
