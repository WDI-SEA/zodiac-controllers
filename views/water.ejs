<div class="water">
    <h1>Water Signs</h1>
    <ul>
        <% waterSigns.forEach(sign => { %>
            <li><a href="/water/<%= sign %>"><%= sign %></a></li>
        <% }) %>
    </ul>
    <ul>
        <% waterTraits.forEach(trait => { %>
            <li> <%= trait %> </li>
        <% }) %>
    </ul>
</div>