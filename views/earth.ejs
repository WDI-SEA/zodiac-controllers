<div class="earth">
    <h1>Earth Signs</h1>
    <ul>
        <% earthSigns.forEach(sign => { %>
            <li><a href="/earth/<%= sign %>"><%= sign %></a></li>
        <% }) %>
    </ul>
    <ul>
        <% earthTraits.forEach(trait => { %>
            <li> <%= trait %> </li>
        <% }) %>
    </ul>
</div>