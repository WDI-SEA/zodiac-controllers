<h1> 🌎 Earth Horoscopes 🌎 </h1>

<%const arrCharacters =  ['Taurus', 'Virgo', 'Capricorn']%>

<%arrCharacters.forEach(char => { %>
    <h3><%= char%></h3>
<% }) %>

<%const arrTraits = ['Grounded', 'Helpful', 'Practical', 'Realistic', 'Materialistic', 'Dependable']%>

<%arrTraits.forEach(traits => { %>
    <li><%= traits%></li>
<% }) %>